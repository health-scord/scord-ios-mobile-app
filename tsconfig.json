{
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "lib": [
      "dom",
      "es6",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext.asynciterable"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "removeComments": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitAny": false,
    "experimentalDecorators": true,
    "target": "es2018",
    "types": []
  }
}
